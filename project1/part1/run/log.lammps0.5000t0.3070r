timestep    ${dt}
timestep    0.002000

pair_style	lj/cut 3.0
pair_coeff	1 1 1.0 1.0 3.0

neighbor	0.3 bin
neigh_modify	delay 0 every 20 check no

fix	1 all nve

thermo ${Dump}
thermo 50

thermo_style custom temp press pe ke etotal

